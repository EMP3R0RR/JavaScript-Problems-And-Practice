
x= 123;
let sign = x < 0 ? -1 : 1;
    x = Math.abs(x);

    let reversed = 0;

    while (x > 0) {
        let digit = x % 10;
        x = Math.floor(x / 10);

        reversed = reversed * 10 + digit;
    }

    reversed = reversed * sign;

    if (reversed < -(2**31) || reversed > (2**31 - 1)) {
        return 0;
    }

    console.log(reversed);